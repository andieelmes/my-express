<%- include('parts/head') -%>
  <body>
    <div class="container-fluid">
      <div class="row">
        <%- include('parts/nav') -%>
        <div class="col-sm-10">
          <h1> <%= title %></h1>

          <ul>
            <% book_list.forEach(book => { %>
              <li>
                <a href="<%= book.url %>"><%= book.title %></a>
                (<%= book.author.name %>)
              </li>
            <% }) %>
          </ul>
        </div>
      </div>
    </div>
  </body>
</html>
